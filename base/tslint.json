{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "prefer-const": true,
    "prettier": [
      true, {
        "singleQuote": true,
        "semi": false,
        "jsxBracketSameLine": false
      }
    ]
  }
}
